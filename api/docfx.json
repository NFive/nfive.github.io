{
  "metadata": [
    {
      "src": [
        {
          "files": [
            "src/NFive/**.csproj"
          ]
        }
      ],
      "dest": "nfive",
      "filter": "filterConfig.yml"
    },
    {
      "src": [
        {
          "files": [
            "src/SDK.Core/**.csproj"
          ]
        }
      ],
      "dest": "core",
      "filter": "filterConfig.yml"
    },
    {
      "src": [
        {
          "files": [
            "src/SDK.Client/**.csproj"
          ]
        }
      ],
      "dest": "client",
      "filter": "filterConfig.yml"
    },
    {
      "src": [
        {
          "files": [
            "src/SDK.Server/**.csproj"
          ]
        }
      ],
      "dest": "server",
      "filter": "filterConfig.yml"
    },
    {
      "src": [
        {
          "files": [
            "src/SDK.Plugins/**.csproj"
          ]
        }
      ],
      "dest": "plugin",
      "filter": "filterConfig.yml"
    }
  ],
  "build": {
    "content": [
      {
        "files": [
          "nfive/**.yml",
          "nfive/index.md"
        ]
      },
      {
        "files": [
          "core/**.yml",
          "core/index.md"
        ]
      },
      {
        "files": [
          "client/**.yml",
          "client/index.md"
        ]
      },
      {
        "files": [
          "server/**.yml",
          "server/index.md"
        ]
      },
      {
        "files": [
          "plugin/**.yml",
          "plugin/index.md"
        ]
      },
      {
        "files": [
          "toc.yml",
          "*.md"
        ]
      }
    ],
    "resource": [],
    "overwrite": [],
    "dest": "_site",
    "globalMetadataFiles": [],
    "fileMetadataFiles": [],
    "template": [
      "default",
      "templates/nfive"
    ],
    "postProcessors": [
      "ExtractSearchIndex"
    ],
    "globalMetadata": {
      "_appName": "NFive",
      "_appTitle": "NFive API Reference",
      "_appFooter": "<span>Copyright Â© 2018-2019 NFive</span>",
      "_enableSearch": "true",
      "_gitContribute": {
        "repo": "https://github.com/NFive/nfive.github.io",
        "branch": "src",
        "apiSpecFolder": "api"
      }
    },
    "xrefService": [
      "https://xref.docs.microsoft.com/query?uid={uid}"
    ],
    "markdownEngineName": "markdig",
    "noLangKeyword": false,
    "keepFileLink": false,
    "cleanupCacheHistory": false,
    "disableGitFeatures": false
  }
}
