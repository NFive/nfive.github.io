{
  "metadata": [
    {
      "src": [
        {
          "files": [
            "src/NFive/**.csproj"
          ]
        }
      ],
      "dest": "nfive",
      "disableGitFeatures": false,
      "disableDefaultFilter": false
    },
    {
      "src": [
        {
          "files": [
            "src/SDK.Core/**.csproj"
          ]
        }
      ],
      "dest": "core",
      "disableGitFeatures": false,
      "disableDefaultFilter": false
    },
    {
      "src": [
        {
          "files": [
            "src/SDK.Client/**.csproj"
          ]
        }
      ],
      "dest": "client",
      "disableGitFeatures": false,
      "disableDefaultFilter": false
    },
    {
      "src": [
        {
          "files": [
            "src/SDK.Server/**.csproj"
          ]
        }
      ],
      "dest": "server",
      "disableGitFeatures": false,
      "disableDefaultFilter": false
    },
    {
      "src": [
        {
          "files": [
            "src/SDK.Plugins/**.csproj"
          ]
        }
      ],
      "dest": "plugin",
      "disableGitFeatures": false,
      "disableDefaultFilter": false
    }
  ],
  "build": {
    "content": [
      {
        "files": [
          "nfive/**.yml",
          "nfive/index.md"
        ]
      },
      {
        "files": [
          "core/**.yml",
          "core/index.md"
        ]
      },
      {
        "files": [
          "client/**.yml",
          "client/index.md"
        ]
      },
      {
        "files": [
          "server/**.yml",
          "server/index.md"
        ]
      },
      {
        "files": [
          "plugin/**.yml",
          "plugin/index.md"
        ]
      },
      {
        "files": [
          "toc.yml",
          "*.md"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "images/**"
        ],
        "exclude": [
          "obj/**",
          "_site/**"
        ]
      }
    ],
    "overwrite": [
      {
        "files": [
          "apidoc/**.md"
        ],
        "exclude": [
          "obj/**",
          "_site/**"
        ]
      }
    ],
    "dest": "_site",
    "globalMetadataFiles": [],
    "fileMetadataFiles": [],
    "template": [
      "default"
    ],
    "postProcessors": [],
    "globalMetadata": {
      "_appTitle": "NFive API Reference",
      "_appFooter": "<span>Copyright Â© 2018-2019 NFive</span>",
      "_enableSearch": "true",
      "_appLogoPath": "images/logo.svg",
      "_appFaviconPath": "images/favicon.png"
    },
    "markdownEngineName": "markdig",
    "noLangKeyword": false,
    "keepFileLink": false,
    "cleanupCacheHistory": false,
    "disableGitFeatures": false
  }
}
